{
    "name": "test/restapi",
    "description": "Test project",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "me"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/skeleton": "7.1.*",
        "symfony/flex": "^2.4",
        "symfony/console": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/http-kernel": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/yaml": "6.4.*",
        "symfony/validator": "6.4.*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "ramsey/uuid": "^4.7",
        "symfony/dotenv": "6.4.*",
        "symfony/messenger": "6.4.*"
    },
    "scripts": {
      "service:deploy-local": [
        "composer install",
        "php bin/console doc:database:create --if-not-exists"
      ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.61",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/phpunit-bridge": "^7.1"
    }
}
